<h1> Users </h1>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Role</th>
      <th>Avatar</th>
      <th> </th>
    </tr>
  </thead>
  <tbody>
<%= for user <- @users do %>
    <tr>
      <td><%= user.user.name %></td>
      <td><%= user.user.email %></td>
      <td><%= user.role.name %></td>
      <td><img src="<%= user.user.avatar_url %>"> </td>
      
    </tr>
<% end %>
  </tbody>
</table>

